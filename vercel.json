{
	"redirects": [
		{
			"source": "/:path*",
			"has": [{ "type": "host", "value": "app.utsuwa.ai" }],
			"destination": "https://utsuwa.ai/app/:path*",
			"permanent": true
		},
		{
			"source": "/:path*",
			"has": [{ "type": "host", "value": "docs.utsuwa.ai" }],
			"destination": "https://utsuwa.ai/docs/:path*",
			"permanent": true
		}
	]
}
